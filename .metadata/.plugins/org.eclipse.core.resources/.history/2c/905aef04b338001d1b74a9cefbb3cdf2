package array;
import java.util.Scanner;
public class Array_03 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("배열 크기 입력 : " );
		int size = scan.nextInt();
		int ar[];
		ar= new int[3];
		for(int i = 0 ; i <= 2; i++) {
			ar[i]=size;
			System.out.println("ar"+i+"="+ size);
			
			
		}
		}
	}


/*
ar[0] 입력 : 25




화면 25 -50 78


*/