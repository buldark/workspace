package for_;

public class Multifor_04_as {

	public static void main(String[] args) {
	
	 int dan,i;
	
	 
	 for(int w = 1 ; w<=8 ;w+=3) {//3ë²ˆ  
		 
		 
		 
		 for(i=1;i<=9;i++) {
				 for (dan=w; dan<=w+2;dan++) {
				 System.out.println(dan+"*"+i+"+"+dan*i);
				 
			 }//fordan
		 
		 }//fori

		 
		 
		 
		 
	 }//forw
	 
	
	}

}
