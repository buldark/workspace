package for_;

public class Multifor_04_as {

	public static void main(String[] args) {
	
	 int dan,i;
	
	 
	 for(int w = 2 ; w<=8 ;w+=3) {//3ë²ˆ   w=1 , w=2 .w=3 =>
		 
		 
		 
		 for(i=1;i<=9;i++) {
				 for (dan=w; dan<=w+2;dan++) {
					 if(dan !=10) System.out.print(dan+"*"+i+"="+dan*i+"\t");
				 
			 }//fordan
			 System.out.println();
		 
		 }//fori
		 System.out.println();
		 	 
	 }//forw
	 
	
	}

}
