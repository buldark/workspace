package multi;
//다차원 배열 파이썬 빅데이터는  4차까지....

public class MultiArray_01 {

	public static void main(String[] args) {
		int[][] ar = new int[3][2];//new는 동적할당 java는 동적할당 해줘야 함 //메모리 활용 더 효율적 가능
		
		
		ar[0][0] = 10;
		ar[0][1] = 20;
		ar[1][0] = 30;
		ar[1][1] = 40;
		ar[2][0] = 50;
		ar[2][1] = 60;
		
		

	}

}
