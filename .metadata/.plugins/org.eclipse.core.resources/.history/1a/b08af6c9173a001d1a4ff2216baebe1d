package multi;

public class MultiArray_02 {

	public static void main(String[] args) {
		int[][] ar = new int [10][10];
		int num =0;
		//입력for
		for(int i = 0 ; i<ar.length;i++) {
			for (int j = 0 ; j<ar[i].length;j++) {
				ar[i][j]= 10*(i+1)+j+1;
				System.out.println(ar[i][j]);
			}
			
		}
		
		
		//출력for

	}

}
